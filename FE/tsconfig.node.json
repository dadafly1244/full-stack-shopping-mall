{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": "./",
    "paths": {
      "#/*": ["./src/*"]
    }
  },
  "include": ["vite.config.ts", "custom.d.ts", "kakao.maps.d.ts"]
}
